<?xml version="1.0" encoding="UTF-8"?>
<root      xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified"
           xmlns="http://www.jpower8.com/newDeveloper/soap"
           targetNamespace="http://www.jpower8.com/newDeveloper/soap">
</filter-poi>
        <post>
            <summary>Filter POIs by params</summary>
            <description></description>
            <parameters>
                <in>header</in>
                <name>X-Execution-Timestamp</name>
                <schema>
                    <type>string</type>
                    <format>date-time</format>
                </schema>
            </parameters>
            <parameters>
                <in>header</in>
                <name>X-Request-Id</name>
                <schema>
                    <type>string</type>
                    <pattern>^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$</pattern>
                </schema>
            </parameters>
            <requestBody>
                <content>
                    <application/json>
                    <schema><$ref>#/components/schemas/RequestData
                    </$ref>
                </schema>
            </application/json>
        </content>
    </requestBody>
    <responses>
        <default>
            <description>An array of POIs</description>
            <content>
                <application/json>
                <schema><$ref>#/components/schemas/POIs
                </$ref>
            </schema>
        </application/json>
    </content>
    </xs:schema>
